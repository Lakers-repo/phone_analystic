<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="configuration.xsl"?>

<configuration>
    <property>
        <!--这里固定写法是writter_kpi的别名,为了其他指标也可以使用，增强代码的健壮性-->
        <!--用户模块下的新增用户-->
        <name>writter_new_user</name>
        <value>com.phone.analystic.mr.newuser.NewUserOutputWriter</value>
    </property>

    <!--浏览器模块下的新增用户-->
    <property>
        <name>writter_browser_new_user</name>
        <value>com.phone.analystic.mr.newuser.NewUserOutputWriter</value>
    </property>

    <!--用户模块下的活跃用户-->
    <property>
        <name>writter_active_user</name>
        <value>com.phone.analystic.mr.activeuser.ActiveUserOutputWriter</value>
    </property>

    <!--浏览器模块下的活跃用户-->
    <property>
        <name>writter_browser_active_user</name>
        <value>com.phone.analystic.mr.activeuser.ActiveUserOutputWriter</value>
    </property>

    <!--用户模块下的活跃会员-->
    <property>
        <name>writter_active_member</name>
        <value>com.phone.analystic.mr.activevipuser.ActiveMemberUserOutputWriter</value>
    </property>

    <!--浏览器模块下的活跃会员-->
    <property>
        <name>writter_browser_active_member</name>
        <value>com.phone.analystic.mr.activevipuser.ActiveMemberUserOutputWriter</value>
    </property>

    <!--用户模块下的新增会员-->
    <property>
        <name>writter_new_member</name>
        <value>com.phone.analystic.mr.vipuser.VipUserOutputWriter</value>
    </property>

    <!--浏览器模块下的新增会员-->
    <property>
        <name>writter_browser_new_member</name>
        <value>com.phone.analystic.mr.vipuser.VipUserOutputWriter</value>
    </property>

    <!--赋值表 新增会员-->
    <property>
        <name>writter_member_info</name>
        <value>com.phone.analystic.mr.vipuser.VipUserOutputWriter</value>
    </property>

    <!--sessionid&sessionlength 用户模块-->
    <property>
        <name>writter_session</name>
        <value>com.phone.analystic.mr.session.SessionOutputWriter</value>
    </property>

    <!--sessionid&sessionlength 浏览器模块-->
    <property>
        <name>writter_browser_session</name>
        <value>com.phone.analystic.mr.session.SessionOutputWriter</value>
    </property>

    <!--pageview-->
    <property>
        <name>writter_pageview</name>
        <value>com.phone.analystic.mr.pageview.PageViewOutputWriter</value>
    </property>

    <!--按小时统计活跃用户-->
    <property>
        <name>writter_hourly_active_user</name>
        <value>com.phone.analystic.mr.activeuser.ActiveUserOutputWriter</value>
    </property>

    <!--local 模块 活跃用户，总会话个数（维度相同），跳出回话个数-->
    <property>
        <name>writter_local</name>
        <value>com.phone.analystic.mr.local.LocalOutputWritable</value>
    </property>
</configuration>
